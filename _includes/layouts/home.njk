<!doctype html>
<html lang="da-DK">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{% if title %}{{ title }} - {% endif %}Inklusio</title>
    <meta name="Description" content="{{ renderData.description or description or metadata.description }}">

    <link rel="stylesheet" href="{{ '/css/my-bootstrap.css' | url }}">
    <link rel="alternate" href="{{ metadata.feed.path | url }}" type="application/atom+xml" title="{{ metadata.title }}">
<link rel="icon" type="image/x-icon" href="{{ '/img/favicon.ico' | url }}">

  </head>
  <body>
  <div class="site-container">
    {% include "layouts/nav.njk" %}

    <main id="main" tabindex="-1">
      {{ content | safe }}
    </main>
     {% include "layouts/footer.njk" %}

 </div>


    <!-- Current page: {{ page.url | url }} -->

</body>

<script src="{{ '/js/inklusio.js' | url }}"></script>

</html>
