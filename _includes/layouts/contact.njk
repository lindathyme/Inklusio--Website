{% set keywords = translations[page.lang].keywords %}

<div class="row">
    <div class="col-12 col-sm-12 col-md-6 col-lg-6 col-xl-5 offset-xl-1">
        <h1>{{ title }}</h1>
        {{ content | safe }}
