---
layout: layouts/page.njk
templateClass: tmpl-contact

eleventyComputed:
  page_lang: "en"
  permalink: "{{ translations[page_lang].navigation.contact.url }}"
  title: "{{ translations[page_lang].navigation.contact.title }}"
---

<!-- Translation Data -->
{% set pageData = translations[page.lang].pages.contact %} {% set keywords =
translations[page.lang].keywords %}

<div class="row mb-5">
  <div class="col-12">
    {% for paragraph in pageData.section[0].paragraph %}
    <p>
      {{ paragraph | safe }}
    </p>
    {% endfor %}

    <h2>{{ pageData.section[1].heading }}</h2>

    {% for paragraph in pageData.section[1].paragraph1 %}
    <p>
      {{ paragraph | safe }}
    </p>
    {% endfor %}

    <p>
      <a href="https://calendly.com/inklusio-ses/1-1" class="btn btn-primary" target="_blank">
        {{ pageData.section[1].call_to_action }}
      </a>
    </p>

    {% for paragraph in pageData.section[1].paragraph2 %}
    <p>
      {{ paragraph | safe }}
    </p>
    {% endfor %}

    <h2>{{ pageData.section[2].heading }}</h2>

    {% for paragraph in pageData.section[2].paragraph %}
    <p>
      {{ paragraph | safe }}
    </p>
    {% endfor %}

    <h2>{{ pageData.section[3].heading }}</h2>

    {% for paragraph in pageData.section[3].paragraph %}
    <p>
      {{ paragraph | safe }}
    </p>
    {% endfor %}
  </div>
</div>
